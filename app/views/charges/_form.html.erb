<%= simple_form_for(@charge) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :service_date %>
    <%= f.association :user %>
    <%= f.input :code %>
    <%= f.association :note %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
